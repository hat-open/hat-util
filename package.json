{
  "private": true,
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^8.57.1",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typedoc": "^0.28.9",
    "typescript": "^5.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src_js/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "preset": "ts-jest/presets/js-with-ts-esm",
    "roots": [
      "test_jest"
    ],
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testEnvironment": "jsdom"
  }
}
