{
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src_js/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "preset": "ts-jest/presets/js-with-ts-esm",
    "roots": [
      "test_jest"
    ],
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testEnvironment": "jsdom"
  }
}
